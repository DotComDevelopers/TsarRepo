@model IEnumerable<TSAR.Models.AccomodationFiles>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.file_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.file_bytes)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.file_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.file_type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.file_bytes)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.accomodationfile_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.accomodationfile_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.accomodationfile_id })
            </td>
        </tr>
    }

</table>
